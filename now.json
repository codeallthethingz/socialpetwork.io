{
  "version": 2,
  "builds": [
    {
      "src": "api/**/index.js",
      "use": "@now/node"
    },
    {
      "src": "www/static/**",
      "use": "@now/static"
    },
    {
      "src": "www/next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ],
  "alias": [
    "www.socialpetwork.io",
    "socialpetwork.io"
  ]
}